<?xml version='1.0' encoding='utf-8'?>
<widget id="edu.munsanMiddleSchool.Japanese" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>일어회화</name>
    <description>
        문산중학교 일본어회화 교육 어플리케이션
    </description>
    <author email="seol311@korea.kr" href="http://www.pajumunsan.ms.kr/">
        설청호
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <engine name="android" spec="~5.2.2" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <plugin name="cordova-plugin-inappbrowser" spec="~1.5.0" />
    <plugin name="cordova-plugin-media" spec="~2.4.0" />
    <plugin name="cordova-plugin-splashscreen" spec="~4.0.0" />
</widget>
